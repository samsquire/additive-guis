			{ "name": "gui-thunks",
				"files": [
					{ "name": "package.json", "contents": "Blah2" }
				],
				"structure": [
					{ name: "method1" },
					{ name: "method2" },
					{ name: "method3" }
				]
			},
			{ "name": "human-query-engine",
				"files": [
					{ "name": "package.json", "contents": "Blah3" }
				],
				"structure": [
					{ name: "method1" },
					{ name: "method2" },
					{ name: "method3" }
				]
			}
		]
	},
	"predicates": [
		"projectArea",
		"editorArea",
		"projectArea leftOf editorArea",
		"projectArea hasSize 3"
	],
	"widgets": {
		"projectArea": {
			"predicates": [
				"projectViewer"
			]
		},
		"projectViewer": {
			"predicates": [
				"folderList",
				"folderList backedBy .folders",
				"folderList mappedTo folderItem",
				"folderList key .name"
			]
		},
		"folderItem": {
			"predicates": [
				"label",
				"folderContents",
				"label hasContent .name",
				"label selects methodList",
				"label emits .structure",
				"folderContents backedBy .files",
				"folderContents mappedTo fileItem",
				"folderContents key .name"
			]
		},
		"fileItem": {
			"predicates": [
				"fileLabel",
				"fileLabel hasContent .name",
				"fileLabel selects editor",
				"fileLabel emits ."
			]
		},
		"structureViewer": {
			"predicates": [
				"methodList",
				"methodList mappedTo methodItem",
				"methodList key .name"
			]
		},
		"methodItem": {
			"predicates": [
				"methodLabel",
				"methodLabel hasContent .name"
			]
		},
		"editorArea": {
			"predicates": [
				"centralArea",
				"structureViewer"	
			]
		},
		"centralArea": {
			"predicates": [
				"feed",
				"console",
				"editor",
				"events",
				"feed above console",
				"events rightOf console",
				"editor mappedTo editText",
				"editor key .name"
				]
		},
		"editText": {
			"predicates": [
				"area",
				"area hasContent .contents"
			]
		}
	}
}